  "scripts": {
    "preinstall": "npx only-allow yarn",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "repl": "npm run start -- --entryFile repl",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate deploy",
    "migrations": "npx prisma migrate dev --name",
    "compile:emails": "ts-node -r tsconfig-paths/register src/cli.ts compile:emails"
  },
